{
  "$schema" : "http://json-schema.org/schema#",
  "$schemaVersion" : "0.0.1",
  "modelTags" : "",
  "$id" : "https://raw.githubusercontent.com/computate-org/smartabyar-smartvillage-static/main/fiware/TrafficSimulation/schema.json",
  "title" : "TrafficSimulation",
  "description" : "A SUMO Traffic Simulation",
  "type" : "object",
  "allOf" : [ {
    "$ref" : "https://smart-data-models.github.io/data-models/common-schema.json#/definitions/GSMA-Commons"
  }, {
    "$ref" : "https://smart-data-models.github.io/data-models/common-schema.json#/definitions/Location-Commons"
  } ],
  "required" : [ "simulationName" ],
  "properties" : {
    "startDateTime" : {
      "description" : "The start date and time. ",
      "type" : "string",
      "format" : "date-time"
    },
    "simulationName" : {
      "type" : "string"
    },
    "sumocfgPath" : {
      "type" : "string"
    },
    "fcdFilePath" : {
      "type" : "string"
    },
    "netFilePath" : {
      "type" : "string"
    },
    "startSeconds" : {
      "description" : "-b, --begin TIME Defines the begin time in seconds; The simulation starts at this time",
      "type" : "number"
    },
    "endSeconds" : {
      "description" : "-e, --end TIME Defines the end time in seconds; The simulation ends at this time",
      "type" : "number"
    },
    "stepSeconds" : {
      "description" : "--step-length TIME Defines the step duration in seconds",
      "type" : "number"
    },
    "paramAvgVehiclePerMinFromWestToEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "30"
    },
    "paramAvgVehiclePerMinFromSouthToNorth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "30"
    },
    "paramVehicleDemandScalingFactor" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "2"
    },
    "paramAvgPedestrianPerMinFromWestToEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "30"
    },
    "paramAvgPedestrianPerMinFromSouthToNorth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "30"
    },
    "paramPedestrianDemandScalingFactor" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "2"
    },
    "paramDemandScale" : {
      "type" : "array"
    },
    "paramMinGreenTimeSecWestEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "50"
    },
    "paramMaxGreenTimeSecWestEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "100"
    },
    "paramMinGreenTimeSecSouthNorth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "50"
    },
    "paramMaxGreenTimeSecSouthNorth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "100"
    },
    "paramPedestrianWaitThresholdSecNorthSouth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "60"
    },
    "paramPedestrianWaitThresholdSecWestEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "60"
    },
    "paramVehicleQueueThresholdWestEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "60"
    },
    "paramVehicleQueueThresholdSouthNorth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "15"
    },
    "paramPedestrianQueueThresholdNorthSouth" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "15"
    },
    "paramPedestrianQueueThresholdWestEast" : {
      "description" : "Min: 1",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "15"
    },
    "paramStepSize" : {
      "description" : "Min: 0.5",
      "type" : "number",
      "minimum" : "0.5",
      "maximum" : "2"
    },
    "paramRunTime" : {
      "description" : "Time duration of each simulated sample path. ",
      "type" : "number",
      "minimum" : "500",
      "maximum" : "3600"
    },
    "paramItersPerPar" : {
      "description" : "Number of repeats per round. ",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "30"
    },
    "paramTotalIterNum" : {
      "description" : "Total iterations to update performance. ",
      "type" : "number",
      "minimum" : "1",
      "maximum" : "30"
    },
    "reportKeys" : {
      "description" : "The generated reports for this simulation",
      "type" : "array"
    },
    "tlsStatesPaths" : {
      "description" : "The paths to all TLS States files",
      "type" : "array"
    },
    "additionalFilePaths" : {
      "description" : "The paths to all the additional SUMO config files",
      "type" : "array"
    },
    "laneAreaDetectorIds" : {
      "type" : "array"
    },
    "laneAreaDetectorLanes" : {
      "type" : "array"
    },
    "laneAreaDetectorPaths" : {
      "type" : "array"
    },
    "e1DetectorIds" : {
      "type" : "array"
    },
    "e1DetectorLanes" : {
      "type" : "array"
    },
    "e1DetectorPaths" : {
      "type" : "array"
    }
  }
}
